{
    "name": "Webpage Theme Selector",
    "version": "1.0.0",
    "description": "Allows for the selection of a webpage theme",
    "manifest_version": 3,
    "author": "Lucas Umberger",
    "action":{
        "default_popup": "popup.html",
        "default_title": "Theme Selector"
    },
    "content_scripts": [
        {
          "js": ["webpage.js"],
          "matches": ["*://*"]
        }
      ],
    "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
    },

    "permissions": ["activeTab"]
}

